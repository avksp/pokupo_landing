{% if data|length > 0 %}
    WParameters.searchResult = {
        {% if data.form|length > 0 %}
            form: {
                {% if data.form.containerId is defined %}
                    container: { widget: '{{ data.form.containerId }}' },
                {% endif %}
                {% if data.form.showCatalog is defined %}
                    showCatalog: {{ data.form.showCatalog }},
                {% endif %}
                {% if data.form.showForm is defined %}
                    showForm: {{ data.form.showForm }},
                {% endif %}
                {% if data.form.useCustomTmpl is defined and data.form.useCustomTmpl == 'true' %}
                    tmpl: {
                        path: '{{ data.form.pathTmpl }}',
                        id: '{{ data.form.idTmpl }}'
                    },
                {% else %}
                    tmpl: {path: '{{protocol ~ '://' ~ url_tmpl_widgets}}/advancedSearchFormTmpl.html'},
                {% endif %}
                animate: function (){}
            },
        {% endif %}
        {% if data.result|length > 0 %}
            content:{
                {% if data.result.containerId is defined %}
                    container: { widget: '{{ data.result.containerId }}' },
                {% endif %}
                {% if data.result.listPerPage is defined %}
                    list: [{{ data.result.listPerPage }}],
                {% endif %}
                {% if data.result.listPerPage is defined %}
                    defaultCount: {{ data.result.defaultPerPage }},
                {% endif %}
                {% if data.result.showCart is defined %}
                    showCart: {{ data.result.showCart }},
                {% endif %}
                {% if data.result.useCustomTmpl is defined  and data.result.useCustomTmpl == 'true' %}
                    tmpl: {
                        path: '{{ data.result.pathTmpl }}',
                        id: { {% for key, value in data.result.idTmpl %}{{ key }}: '{{ value }}'{% if not loop.last %},{% endif %}{% endfor %} }
                    },
                {% else %}
                    tmpl: {path: '{{protocol ~ '://' ~ url_tmpl_widgets}}/searchResultTmpl.html'},
                {% endif %}
                animate: function (){}
            }
        {% endif %}
    }
{% endif %}


