{% if data|length > 0 %}
    WParameters.order = {
    {% if data.containerId is defined %}
        container: { widget: '{{ data.containerId }}' },
    {% endif %}
    {% if data.useCustomTmpl is defined %}
        tmpl: {
        path: '{{ data.pathTmpl }}',
        id: { {% for key, value in data.idTmpl %}{{ key }}: '{{ value }}'{% if not loop.last %},{% endif %}{% endfor %} }
        },
    {% else %}
        tmpl: {path: '{{protocol ~ '://' ~ url_tmpl_widgets}}/orderTmpl.html'},
    {% endif %}
    animate: function (){}
    }
{% endif %}