{% if data|length > 0 %}
    WParameters.menuPersonalCabinet = {
        {% if data.containerId is defined %}
            container: { widget: '{{ data.containerId }}' },
        {% endif %}
        {% if data.showCart is defined %}
            showCart: {{ data.showCart }},
        {% endif %}
        {% if data.showRegSeller is defined %}
            showRegSeller: {{ data.showRegSeller }},
        {% endif %}
        {% if data.useCustomTmpl is defined %}
            tmpl: { path: '{{ data.pathTmpl }}', id: '{{ data.idTmpl }}'},
        {% else %}
            tmpl: {path: '{{protocol ~ '://' ~ url_tmpl_widgets}}/menuPersonalCabinetTmpl.html'},
        {% endif %}
        animate: function (){}
    }
{% endif %}